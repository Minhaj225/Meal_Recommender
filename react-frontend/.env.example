# Environment Configuration for Meal Recommender Frontend

# Backend API URL (Node.js/Express server)
VITE_API_URL=http://localhost:5001/api

# ML API URL (Python Flask server)
VITE_ML_API_URL=http://localhost:5000

# App Configuration
VITE_APP_NAME=AI Meal Recommender
VITE_APP_VERSION=2.0.0

# Feature Flags
VITE_ENABLE_ML_PREDICTIONS=true
VITE_ENABLE_ADMIN_FEATURES=true
VITE_ENABLE_ANALYTICS=true

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_API_CALLS=true

# Production Settings (uncomment for production)
# VITE_API_URL=https://your-backend-domain.com/api
# VITE_ML_API_URL=https://your-ml-api-domain.com
# VITE_DEBUG_MODE=false
# VITE_LOG_API_CALLS=false
